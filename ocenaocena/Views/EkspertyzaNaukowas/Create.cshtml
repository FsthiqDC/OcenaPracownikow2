@model ocenaocena.Models.EkspertyzaNaukowa

@{
    ViewData["Title"] = "Create";
}

<h4>Dodaj ekspertyze naukową</h4>
<hr />
<div class="row col-md-16 interspace justify-content-center">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NazwaInstytucjiZlecajacej" class="control-label">Nazwa instytucji zlecającej</label>
                <input asp-for="NazwaInstytucjiZlecajacej" class="form-control" />
                <span asp-validation-for="NazwaInstytucjiZlecajacej" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TytulEkspertyzyOpracowaniaNaukowego" class="control-label">Tytuł opraocowania naukowego</label>
                <input asp-for="TytulEkspertyzyOpracowaniaNaukowego" class="form-control" />
                <span asp-validation-for="TytulEkspertyzyOpracowaniaNaukowego" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TerminPrzekazania" class="control-label">Termin przekazania</label>
                <input asp-for="TerminPrzekazania" class="form-control" />
                <span asp-validation-for="TerminPrzekazania" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Dodaj" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
