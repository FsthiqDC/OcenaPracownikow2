@model IEnumerable<ocenaocena.Models.EkspertyzaNaukowa>

@{
    ViewData["Title"] = "Index";
}


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NazwaInstytucjiZlecajacej)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TytulEkspertyzyOpracowaniaNaukowego)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TerminPrzekazania)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NazwaInstytucjiZlecajacej)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TytulEkspertyzyOpracowaniaNaukowego)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TerminPrzekazania)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EkspertyzaNaukowaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EkspertyzaNaukowaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EkspertyzaNaukowaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
